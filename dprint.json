{
  "indentWidth": 2,
  "roslyn": {
    "indentWidth": 4,
    "newLineKind": "crlf"
  },
  "exec": {
    "commands": [{
      "command": "rustfmt --config imports_granularity=item --edition 2021",
      "exts": ["rs"]
    }]
  },
  "excludes": [
    "**/*-lock.json",
    "**/obj",
    "**/bin",
    "**/target",
    "**/.vs"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.95.13.wasm",
    "https://plugins.dprint.dev/json-0.21.0.wasm",
    "https://plugins.dprint.dev/markdown-0.20.0.wasm",
    "https://plugins.dprint.dev/roslyn-0.18.0.json@9586754a7e586ccda5dba4e694c0f225aabde63720c905dd85e02453b6f5e5de",
    "https://plugins.dprint.dev/prettier-0.62.0.json@80fefd1450bfd3f469590cc6a3f40f6ace26974f948b9602124f486036fbdfab",
    "https://plugins.dprint.dev/exec-0.6.0.json@a054130d458f124f9b5c91484833828950723a5af3f8ff2bd1523bd47b83b364"
  ]
}
