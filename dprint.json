{
  "incremental": true,
  "indentWidth": 2,
  "roslyn": {
    "indentWidth": 4,
    "newLineKind": "crlf"
  },
  "exec": {
    "associations": "**/*.rs",
    "rustfmt": "rustfmt --config imports_granularity=item --edition 2021"
  },
  "excludes": [
    "**/*-lock.json",
    "**/obj",
    "**/bin",
    "**/target",
    "**/.vs"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.66.0.wasm",
    "https://plugins.dprint.dev/json-0.15.0.wasm",
    "https://plugins.dprint.dev/markdown-0.13.0.wasm",
    "https://plugins.dprint.dev/roslyn-0.13.0.json@2bfa2e67a02b84aa2d23f78ce0f00149cd681078a4a646cf546af804e632e6cf",
    "https://plugins.dprint.dev/prettier-0.26.1.json@fdbe31f6aecd24f9d6b924214710a6766050d03146163b4e241e6056b2462f2e",
    "https://plugins.dprint.dev/exec-0.3.5.json@d687dda57be0fe9a0088ccdaefa5147649ff24127d8b3ea227536c68ee7abeab"
  ]
}
