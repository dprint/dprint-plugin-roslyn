{
  "indentWidth": 2,
  "roslyn": {
    "indentWidth": 4,
    "newLineKind": "crlf"
  },
  "exec": {
    "commands": [{
      "command": "rustfmt --config imports_granularity=item --edition 2021",
      "exts": ["rs"]
    }]
  },
  "excludes": [
    "**/*-lock.json",
    "**/obj",
    "**/bin",
    "**/target",
    "**/.vs"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.88.9.wasm",
    "https://plugins.dprint.dev/json-0.19.1.wasm",
    "https://plugins.dprint.dev/markdown-0.16.3.wasm",
    "https://plugins.dprint.dev/roslyn-0.17.0.json@e234744fa209f46a86e14a1e4fba5b22dc0c3486a211c1dd0e97464437a771ab",
    "https://plugins.dprint.dev/prettier-0.32.1.json@19aa403ef0862ba8c41164e3dc6f84c0b7a66c2b11e42726b23dd25e6302ada9",
    "https://plugins.dprint.dev/exec-0.4.4.json@c207bf9b9a4ee1f0ecb75c594f774924baf62e8e53a2ce9d873816a408cecbf7"
  ]
}
