{
  "incremental": true,
  "indentWidth": 2,
  "roslyn": {
    "indentWidth": 4,
    "newLineKind": "crlf"
  },
  "includes": ["**/*.{json,md,cs,js}"],
  "excludes": [
    "**/*-lock.json",
    "**/obj",
    "**/bin"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.65.1.wasm",
    "https://plugins.dprint.dev/json-0.14.1.wasm",
    "https://plugins.dprint.dev/markdown-0.12.2.wasm",
    "https://plugins.dprint.dev/roslyn-0.6.0.exe-plugin@a8885422bfacead80df8dfe9cdde9d3cca70b410207331dc1b16534441e69de8"
  ]
}
