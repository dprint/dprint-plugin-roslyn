{
  "incremental": true,
  "indentWidth": 2,
  "roslyn": {
    "indentWidth": 4,
    "newLineKind": "crlf"
  },
  "includes": ["**/*.{json,md,cs,js,ts}"],
  "excludes": [
    "**/*-lock.json",
    "**/obj",
    "**/bin"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.66.0.wasm",
    "https://plugins.dprint.dev/json-0.15.0.wasm",
    "https://plugins.dprint.dev/markdown-0.13.0.wasm",
    "https://plugins.dprint.dev/roslyn-0.6.3.exe-plugin@5ee82fe51bfe6b3bf9f91d68512386a2010dc5013ceeba3b4f2c46b9bfc319c8"
  ]
}
